{
	"python.jediEnabled": false,
	"html.format.preserveNewLines": false,
	"html.format.indentInnerHtml": true,
	"languageserver": {
		"ocaml": {
			"command": "~/.opam/default/bin/ocamllsp",
			"filetypes": [
				"ocaml"
			]
